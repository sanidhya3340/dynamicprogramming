#include<bits/stdc++.h>
using namespace std;

bool t[502][502];

bool subset(int wt[],int w,int n){
    if(n==0 || w==0){
        return 0;
    }
    for(int i=0;i<n+1;i++){
        for(int j=0;j<w+1;j++){
            if(j==0){
                t[i][j]=true;
            }
            else if(i==0){
                t[i][j]=false;
            }
        }
    }

    
    for(int i=1;i<n+1;i++){
        for(int j=1;j<w+1;j++){
            if(wt[i-1]<=j){
                t[i][j]= t[i][j-wt[i-1]] || t[i-1][j];
            }
            else if(wt[i-1]>j){
                t[i][j]=t[i-1][j];
            }
        }
    }

    for(int i=0;i<n+1;i++){
        for(int j=0;j<w+1;j++){
            cout<<t[i][j]<<" ";
        }
        cout<<endl;
    }


    return t[n][w];
}

int main(){
    int wt[]={2,4,7,8,10};

    int w = 1;

    int n = 5;

    cout<<subset(wt,w,n);
}
